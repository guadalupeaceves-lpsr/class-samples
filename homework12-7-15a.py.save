import turtle
length = 100

def myAbstract(myTurtle,side):
	myTurtle.forward(50)
	myTurtle.right(60)
	myTurtle.forward(50)
        myTurtle.right(60)
        myTurtle.forward(50)
        myTurtle.right(60)
        myTurtle.forward(50)
        myTurtle.right(60)
        myTurtle.forward(50)
        myTurtle.right(60)
        myTurtle.forward(50)
        myTurtle.right(60)

art = turtle.Turtle()

while length > 0:
	myAbstract(art, length)
	art.circle(5)
	art.right(20)
	myAbstract(art,length)
	art.left(15)
	length = length + 1	

art.exitonclick()
